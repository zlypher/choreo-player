{
  "name": "choreo-player",
  "version": "1.0.0",
  "description": "Choreo Player is a music player with helpful functionality to help study choreographies.",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "concurrently --kill-others \"npm run build:watch\" \"npm run server\"",
    "server": "http-server ."
  },
  "keywords": [
    "music-player",
    "choreography",
    "web-audio-api"
  ],
  "homepage": "https://zlypher.github.io/choreo-player/",
  "repository": {
    "type": "git",
    "url": "https://github.com/zlypher/choreo-player.git"
  },
  "author": "Thomas Prochazka <thomas.prochazka@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "http-server": "0.11.1",
    "rollup": "^0.58.2",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
